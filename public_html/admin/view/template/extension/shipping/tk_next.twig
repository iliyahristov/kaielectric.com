        color: #666;
        background-color: #fff;
        border-color: #ddd;
    }

    .button-label input:checked + span {
        color: #fff;
        background-color: #3c933c;
        border-color: #2f722f;
    }

    .button-label input:checked + span.btn-danger {
        color: #fff;
        background-color: #e3503e;
        border-color: #dd3520;
    }

    .button-label i {
        font-size: 15px;
        transition: 0.5s all;
    }
</style>

<script>
	{% for next_country in next_countries %}
	  $('.input-{{ next_country.code }}-status').on('change', function () {
	      if ($(this).val() > 0) {
	          $('#box-{{ next_country.code }}-status').fadeIn();
	      } else {
	          $('#box-{{ next_country.code }}-status').fadeOut();
	      }
	  });
 
		{% for courier in next_country.couriers_in_detail %}
		  $('.input-{{ next_country.code }}-{{ courier.id }}-status').on('change', function () {
		      if ($(this).val() > 0) {
		          $('#box-{{ next_country.code }}-{{ courier.id }}-status').fadeIn();
		      } else {
		          $('#box-{{ next_country.code }}-{{ courier.id }}-status').fadeOut();
		      }
		  });

		  $('.input-{{ next_country.code }}-{{ courier.id }}-option').on('change', function () {
		      if ($(this).val() != 0) {
		          $('#box-{{ next_country.code }}-{{ courier.id }}-option').fadeIn();
		      } else {
		          $('#box-{{ next_country.code }}-{{ courier.id }}-option').fadeOut();
		      }
		  });

		  $('.input-{{ next_country.code }}-{{ courier.id }}-address-status').on('change', function () {
		      if ($(this).val() != 0) {
		          $('#box-{{ next_country.code }}-{{ courier.id }}-address-status').fadeIn();
		      } else {
              $('#box-{{ next_country.code }}-{{ courier.id }}-address-status').fadeOut();
		      }
		  });

		  $('.input-{{ next_country.code }}-{{ courier.id }}-office-status').on('change', function () {
		      if ($(this).val() != 0) {
		          $('#box-{{ next_country.code }}-{{ courier.id }}-office-status').fadeIn();
		      } else {
		          $('#box-{{ next_country.code }}-{{ courier.id }}-office-status').fadeOut();
		      }
		  });

      $('.input-{{ next_country.code }}-{{ courier.id }}-machine-status').on('change', function () {
	      if ($(this).val() != 0) {
	          $('#box-{{ next_country.code }}-{{ courier.id }}-machine-status').fadeIn();
	      } else {
	          $('#box-{{ next_country.code }}-{{ courier.id }}-status-status').fadeOut();
	      }
      });
		{% endfor %}
	{% endfor %}


  function nextButtonLogin() {
      $('#loading').remove();
      $('#login_error').html('').removeClass("text-danger");
      $('#login_button').after('<span id="loading" class="attention" style="padding: 5px;">Моля, изчакайте.</span>');

      $.ajax({
          url: 'index.php?route=extension/shipping/tk_next/login&user_token={{ user_token }}',
          type: 'POST',
          data: 'app_id=' + encodeURIComponent($('#app_id').val()) + '&app_secret=' + encodeURIComponent($('#app_secret').val()) + '&sender_id=' + encodeURIComponent($('#sender_id').val()),
          dataType: 'json',
          success: function (data) {
              if (data) {
                  $('#loading').remove();

                  if (data.success) {
                      window.location = window.location;
                  } else {
                      $('#login_error').html(data.message).addClass('text-danger');
                  }
              }
          },
          error: function (request) {
              $('#loading').remove();

              $('#login_error').html('Login Error').addClass("text-danger");
          }
      });
  }

	function nextButtonLogout() {
			if (confirm('{{ text_warning_logout }}')) {
					$.get('index.php?route=extension/shipping/tk_next/logout&user_token={{ user_token }}').done(function () {
							window.location = window.location;
					});
			}
    }

</script>
{{ footer }}