{{ header }}{{ column_left }} 
{% set weight_value_row = 0 %}
<div id="content">
	<div class="page-header">
		<div class="container-fluid">
			<div class="pull-right">
				{% if (shipping_tk_next_logged) %}
					<button type="submit" form="form-next" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary">
						<i class="fa fa-save"></i></button>
				{% endif %}
				<a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a>
			</div>
			<h1>{{ heading_title }}</h1>
			<ul class="breadcrumb">
				{% for breadcrumb in breadcrumbs %}
					<li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
				{% endfor %}
			</ul>
		</div>
	</div>
	<div class="container-fluid">
		
		{% if (error_warning) %}
			<div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
				<button type="button" class="close" data-dismiss="alert">&times;</button>
			</div>
		{% endif %}
		
		{% if (success) %}
			<div class="alert alert-success alert-dismissible"><i class="fa fa-check-circle"></i> {{ success }}
				<button type="button" class="close" data-dismiss="alert">&times;</button>
			</div>
		{% endif %}
		
		<form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-next" class="form-horizontal">
			
			{% if module_tk_checkout_token %}
				
				{% if shipping_tk_next_logged %}
					<div class="panel panel-default">
						<div class="panel-body">
							<div class="form-group required">
								<label class="col-sm-3 control-label" for="app_id">APP ID</label>
								<div class="col-sm-9">
									<input type="text" id="app_id" name="shipping_tk_next_app_id" value="{{ shipping_tk_next_app_id }}" placeholder="APP ID" class="form-control" disabled/>
								</div>
							</div>
							<div class="form-group required">
								<label class="col-sm-3 control-label" for="app_secret">APP SECRET</label>
								<div class="col-sm-9">
									<input type="text" id="app_secret" name="shipping_tk_next_app_secret" value="{{ shipping_tk_next_app_secret }}" placeholder="APP SECRET" class="form-control" disabled/>
								</div>
							</div>
							<div class="form-group required">
								<label class="col-sm-3 control-label" for="app_secret">SENDER ID</label>
								<div class="col-sm-9">
									<input type="text" id="app_secret" name="shipping_tk_next_sender_id" value="{{ shipping_tk_next_sender_id }}" placeholder="SENDER ID" class="form-control" disabled/>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label"></label>
								<div class="col-sm-9">
									<a onclick="nextButtonLogout();" class="btn btn-primary"><span>{{ button_logout }}</span></a>
								</div>
							</div>
							<hr>
							<div class="form-group">
								<div class="col-sm-3 text-right"><p><b>Крон за ъпдейт на пратки:</b></p></div>
								<div class="col-sm-9">
									<p>/opt/cpanel/ea-php74/root/usr/bin/php {{ root }}
										catalog/controller/tk_checkout/cron_next_shipping.php {{ web_url }} > /dev/null 2>&1</p>
								</div>
							</div>
						</div>
					</div>
				{% endif %}
				
				{% if not shipping_tk_next_logged %}
					
					<div class="panel panel-default">
						<div class="panel-body">
							<div class="form-group required">
								<label class="col-sm-3 control-label" for="app_id">APP ID</label>
								<div class="col-sm-9">
									<input type="text" id="app_id" name="shipping_tk_next_app_id" value="{{ shipping_tk_next_app_id }}" placeholder="APP ID" class="form-control"/>
									{% if (error_app_id) %}
										<span class="text-danger">{{ error_app_id }}</span>
									{% endif %}
								</div>
							</div>
							<div class="form-group required">
								<label class="col-sm-3 control-label" for="app_secret">APP SECRET</label>
								<div class="col-sm-9">
									<input type="text" id="app_secret" name="shipping_tk_next_app_secret" value="{{ shipping_tk_next_app_secret }}" placeholder="APP SECRET" class="form-control"/>
									{% if (error_app_secret) %}
										<span class="text-danger">{{ error_app_secret }}</span>
									{% endif %}
								</div>
							</div>
							<div class="form-group required">
								<label class="col-sm-3 control-label" for="app_secret">SENDER ID</label>
								<div class="col-sm-9">
									<input type="text" id="sender_id" name="shipping_tk_next_sender_id" value="{{ shipping_tk_next_sender_id }}" placeholder="SENDER ID" class="form-control"/>
									{% if (error_sender_id) %}
										<span class="text-danger">{{ error_sender_id }}</span>
									{% endif %}
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label"></label>
								<div class="col-sm-9">
									<a id="login_button" onclick="nextButtonLogin();" class="btn btn-primary"><span id="login_text">{{ button_login }}</span></a>
									<span id="login_error"></span>
								</div>
							</div>
						</div>
					</div>
					
				{% else %}
					
					<div class="panel panel-default">
						<div class="panel-body" id="additional_table">
							
							<div class="col-sm-2">
								<ul class="nav nav-pills nav-stacked">
									<li class="active">
										<a href="#v-pills-settings" data-toggle="tab">{{ text_settings }}</a>
									</li>
									<li>
										<a href="#v-pills-statuses" data-toggle="tab">{{ text_statuses }}</a>
									</li>
									{% for next_country in next_countries %}
										<li>
											<a href="#v-pills-{{ next_country.code }}" data-toggle="tab">{{ next_country.name }}</a>
										</li>
									{% endfor %}
								</ul>
							</div>
							
							<div class="col-sm-10">
								<div class="tab-content">
									
									<div class="tab-pane fade in active" id="v-pills-settings">
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="shipping_tk_next_status">Статус</label>
											<div class="col-sm-9">
												<select class="form-control" id="shipping_tk_next_status" name="shipping_tk_next_status">
													{% if (shipping_tk_next_status) %}
														<option value="1" selected="selected">{{ text_enabled }}</option>
														<option value="0">{{ text_disabled }}</option>
													{% else %}
														<option value="1">{{ text_enabled }}</option>
														<option value="0" selected="selected">{{ text_disabled }}</option>
													{% endif %}
												</select>
											</div>
										</div>
										
										{% if custom_fields %}
											<div class="form-group">
												<label class="col-sm-3 control-label" for="shipping_tk_next_company">Поле за име на фирма</label>
												<div class="col-sm-9">
													<select name="shipping_tk_next_company" id="shipping_tk_next_company" class="form-control">
														<option value="9999">{{ text_no_company}}</option>
														{% for custom_field in custom_fields %}
															{% if custom_field.custom_field_id == shipping_tk_next_company %}
																<option value="{{ custom_field.custom_field_id }}" selected="selected">{{ custom_field.name }}</option>
															{% else %}
																<option value="{{ custom_field.custom_field_id }}">{{ custom_field.name }}</option>
															{% endif %}
														{% endfor %}
													</select>
												</div>
											</div>
										{% endif %}
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="shipping_tk_next_geo_zone_id">{{ text_geo_zone }}</label>
											<div class="col-sm-9">
												<select class="form-control" id="shipping_tk_next_geo_zone_id" name="shipping_tk_next_geo_zone_id">
													<option value="0">{{ text_all_zones }}</option>
													{% for geo_zone in geo_zones %}
														{% if (geo_zone['geo_zone_id'] == shipping_tk_next_geo_zone_id) %}
															<option value="{{ geo_zone['geo_zone_id'] }}" selected="selected">{{ geo_zone['name'] }}</option>
														{% else %}
															<option value="{{ geo_zone['geo_zone_id'] }}">{{ geo_zone['name'] }}</option>
														{% endif %}
													{% endfor %}
												</select>
											</div>
										</div>
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="input-tax-class">Данъчен клас</label>
											<div class="col-sm-9">
												<select name="shipping_tk_next_tax_class_id" id="input-tax-class" class="form-control">
													<option value="0">{{ text_none }}</option>
													{% for tax_class in tax_classes %}
														{% if tax_class.tax_class_id == shipping_tk_next_tax_class_id %}
															<option value="{{ tax_class.tax_class_id }}" selected="selected">{{ tax_class.title }}</option>
														{% else %}
															<option value="{{ tax_class.tax_class_id }}">{{ tax_class.title }}</option>
														{% endif %}
													{% endfor %}
												</select>
											</div>
										</div>
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="shipping_tk_next_sort_order">{{ text_sort_order }}</label>
											<div class="col-sm-9">
												<input class="form-control" type="text" id="shipping_tk_next_sort_order" name="shipping_tk_next_sort_order" value="{{ shipping_tk_next_sort_order }}" size="1" />
											</div>
										</div>
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="shipping_tk_next_status">Разреши дебъг функция</label>
											<div class="col-sm-9">
												<select class="form-control" id="shipping_tk_next_debug" name="shipping_tk_next_debug">
													{% if (shipping_tk_next_debug) %}
														<option value="1" selected="selected">{{ text_enabled }}</option>
														<option value="0">{{ text_disabled }}</option>
													{% else %}
														<option value="1">{{ text_enabled }}</option>
														<option value="0" selected="selected">{{ text_disabled }}</option>
													{% endif %}
												</select>
											</div>
										</div>
									
									</div>
									
									<div class="tab-pane" id="v-pills-statuses">
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="shipping_tk_next_order_status_id">{{ text_statuses_10 }}:</label>
											<div class="col-sm-9">
												<select name="shipping_tk_next_order_status_id" id="shipping_tk_next_order_status_id" class="form-control">
													<option value="0">{{ text_statuses_9 }}</option>
													{% for order_status in order_statuses %}
														
														{% if (order_status['order_status_id'] == shipping_tk_next_order_status_id) %}
															<option value="{{ order_status['order_status_id'] }}" selected="selected">{{ order_status['name'] }}</option>
														{% else %}
															<option value="{{ order_status['order_status_id'] }}">{{ order_status['name'] }}</option>
														{% endif %}
													{% endfor %}
												</select>
											</div>
										</div>
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="input-shipping_tk_next_order_status_id_mail">{{ text_statuses_8 }}:</label>
											<div class="col-sm-9">
												<select name="shipping_tk_next_order_status_id_mail" id="input-shipping_tk_next_order_status_id_mail" class="form-control">
													{% if  shipping_tk_next_order_status_id_mail %}
														<option value="0">{{ text_disabled }}</option>
														<option value="1" selected="selected">{{ text_enabled }}</option>
													{% else %}
														<option value="0" selected="selected">{{ text_disabled }}</option>
														<option value="1">{{ text_enabled }}</option>
													{% endif %}
												</select>
											</div>
										</div>
										
										<div class="form-group">
											<label class="col-sm-3 control-label" >{{ text_statuses_1 }}<br> {shipment_number} - {{ text_statuses_2 }}:</label>
											<div class="col-sm-9">
												<textarea name="shipping_tk_next_order_status_id_mail_text" rows="3" class="form-control">{{ shipping_tk_next_order_status_id_mail_text }}</textarea>
											</div>
										</div>
										
										<div class="form-group">
											<div class="col-sm-3 text-right"></div>
											<div class="col-sm-9">
												<br><br><h4><b>{{ text_statuses_3 }}</b></h4>
											</div>
										</div>
										
										{% for key,text_status in text_label_status %}
											<div class="form-group">
												<label class="col-sm-3 control-label">{{ key }} - {{ text_status }}</label>
												
												<div class="col-sm-9">
													<div class="row">
														<div class="col-sm-3">{{ text_statuses_4 }}</div>
														<div class="col-sm-3">
															<select name="shipping_tk_next_order_status[{{ key }}]" id="input-shipping_tk_next_order_status" class="form-control">
																<option value="">{{ text_statuses_5 }}</option>
																{% for order_status in order_statuses %}
																	{% if  shipping_tk_next_order_status[key] == order_status.order_status_id %}
																		<option value="{{ order_status.order_status_id }}" selected="selected">{{ order_status.name }}</option>
																	{% else %}
																		<option value="{{ order_status.order_status_id }}">{{ order_status.name }}</option>
																	{% endif %}
																{% endfor %}
															</select>
														</div>
														<div class="col-sm-3">{{ text_statuses_6 }}</div>
														<div class="col-sm-3">
															<select name="shipping_tk_next_order_status_mail[{{ key }}]" id="input-shipping_tk_next_order_status_mail" class="form-control">
																{% if  shipping_tk_next_order_status_mail[key] %}
																	<option value="0">{{ text_disabled }}</option>
																	<option value="1" selected="selected">{{ text_enabled }}</option>
																{% else %}
																	<option value="0" selected="selected">{{ text_disabled }}</option>
																	<option value="1">{{ text_enabled }}</option>
																{% endif %}
															</select>
														</div>
														<div class="clearfix"></div>
														<br>
														
														<div class="col-sm-3">{{ text_statuses_7 }}</div>
														<div class="col-sm-9">
															<textarea name="shipping_tk_next_order_status_mail_text[{{ key }}]" rows="3" placeholder="Въведи текст" id="input-shipping_tk_next_order_status_mail_text" class="form-control">{{ shipping_tk_next_order_status_mail_text[key] }}</textarea>
														</div>
													</div>
												</div>
											</div>
										{% endfor %}
									
									</div>
									
									
									{% for next_country in next_countries %}
										<div class="tab-pane fade" id="v-pills-{{ next_country.code }}">
											
											<div class="form-group">
												<div class="col-sm-12">
													<h3>{{ next_country.name }}
														<small>{{ text_code }}:{{ next_country.code }}</small> <small>{{ text_currency }}
															:{{ next_country.currency }}</small>
													</h3>
												</div>
											</div>
											
											<input type="hidden" name="shipping_tk_next_countries[{{ next_country.code }}][id]" value="{{ next_country.id }}"/>
											<input type="hidden" name="shipping_tk_next_countries[{{ next_country.code }}][code]" value="{{ next_country.code }}"/>
											<input type="hidden" name="shipping_tk_next_countries[{{ next_country.code }}][currency]" value="{{ next_country.currency }}"/>
											<input type="hidden" name="shipping_tk_next_countries[{{ next_country.code }}][name]" value="{{ next_country.name }}"/>
											
											<div class="form-group">
												<label class="col-sm-1 control-label">{{ text_status }}</label>
												<div class="col-sm-11">
													{% if shipping_tk_next_countries[next_country.code]['status'] %}
														<label class="button-label">
															<input type="radio" name="shipping_tk_next_countries[{{ next_country.code }}][status]" value="0" class="input-{{ next_country.code }}-status"/>
															<span class="btn btn-default">{{ text_disabled }}</span> </label>
														<label class="button-label">
															<input type="radio" name="shipping_tk_next_countries[{{ next_country.code }}][status]" value="1" checked="checked" class="input-{{ next_country.code }}-status"/>
															<span class="btn btn-default">{{ text_enabled }}</span> </label>
													{% else %}
														<label class="button-label">
															<input type="radio" name="shipping_tk_next_countries[{{ next_country.code }}][status]" value="0" checked="checked" class="input-{{ next_country.code }}-status"/>
															<span class="btn btn-default">{{ text_disabled }}</span> </label>
														<label class="button-label">
															<input type="radio" name="shipping_tk_next_countries[{{ next_country.code }}][status]" value="1" class="input-{{ next_country.code }}-status"/>
															<span class="btn btn-default">{{ text_enabled }}</span> </label>
													{% endif %}
												</div>
											</div>
									
											<div id="box-{{ next_country.code }}-status" {% if shipping_tk_next_countries[next_country.code]['status'] %}style="display: block" {% else %}style="display: none"{% endif %}>
												
												
												<ul class="nav nav-tabs">
													{% for key,courier in next_country.couriers_in_detail %}
													<li class="{% if key == 0 %}active{% endif %}">
														<a href="#tab-{{ next_country.code }}-{{ courier.id }}" data-toggle="tab">{{ courier.name }}</a>
													</li>
													{% endfor %}
												</ul>
												
												
												<div class="tab-content">
													
													{% for key,courier in next_country.couriers_in_detail %}
													<div class="tab-pane {% if key == 0 %}active{% endif %}" id="tab-{{ next_country.code }}-{{ courier.id }}">
														
														<input type="hidden" name="shipping_tk_next_countries[{{ next_country.code }}][couriers][{{ courier.id }}][id]" value="{{ courier.id }}"/>
														<input type="hidden" name="shipping_tk_next_countries[{{ next_country.code }}][couriers][{{ courier.id }}][name]" value="{{ courier.name }}"/>
													
														<div class="form-group">
														<h4 class="col-sm-1 control-label">{{ courier.name }}</h4>
														<div class="col-sm-11">
															{% if shipping_tk_next_countries[next_country.code]['couriers'][courier.id]['status'] %}
																<label class="button-label">
																	<input type="radio" name="shipping_tk_next_countries[{{ next_country.code }}][couriers][{{ courier.id }}][status]" value="0" class="input-{{ next_country.code }}-{{ courier.id }}-status"/>
																	<span class="btn btn-default">{{ text_disabled }}</span> </label>
																<label class="button-label">
																	<input type="radio" name="shipping_tk_next_countries[{{ next_country.code }}][couriers][{{ courier.id }}][status]" value="1" checked="checked" class="input-{{ next_country.code }}-{{ courier.id }}-status"/>
																	<span class="btn btn-default">{{ text_enabled }}</span> </label>
															{% else %}
																<label class="button-label">
																	<input type="radio" name="shipping_tk_next_countries[{{ next_country.code }}][couriers][{{ courier.id }}][status]" value="0" checked="checked" class="input-{{ next_country.code }}-{{ courier.id }}-status"/>
																	<span class="btn btn-default">{{ text_disabled }}</span> </label>
																<label class="button-label">
																	<input type="radio" name="shipping_tk_next_countries[{{ next_country.code }}][couriers][{{ courier.id }}][status]" value="1" class="input-{{ next_country.code }}-{{ courier.id }}-status"/>
																	<span class="btn btn-default">{{ text_enabled }}</span> </label>
															{% endif %}
														</div>
													</div>
			
														<div id="box-{{ next_country.code }}-{{ courier.id }}-status" {% if shipping_tk_next_countries[next_country.code]['couriers'][courier.id]['status'] %}style="display: block" {% else %}style="display: none"{% endif %}>
															
															
															<ul class="nav nav-tabs">
																<li class="active">
																		<a href="#tab-{{ next_country.code }}-{{ courier.id }}-settings" data-toggle="tab">{{ text_settings }}</a>
																</li>
																{% if courier.to_address %}
																	<li>
																		<a href="#tab-{{ next_country.code }}-{{ courier.id }}-address" data-toggle="tab">{{ text_address }}</a>
																	</li>
																{% endif %}
																{% if courier.to_office %}
																	<li>
																		<a href="#tab-{{ next_country.code }}-{{ courier.id }}-office" data-toggle="tab">{{ text_office }}</a>
																	</li>
																	<li>
																		<a href="#tab-{{ next_country.code }}-{{ courier.id }}-machine" data-toggle="tab">{{ text_machine }}</a>
																	</li>
																{% endif %}
															</ul>
															
															<div class="tab-content">
															
															<div class="tab-pane active" id="tab-{{ next_country.code }}-{{ courier.id }}-settings">
															
																<div class="form-group">
																	<label class="col-sm-3 control-label">{{ text_payer }}</label>
																	<div class="col-sm-9">
																		<label class="button-label">
																			<input type="radio" name="shipping_tk_next_countries[{{ next_country.code }}][couriers][{{ courier.id }}][payer]" value="receiver" {% if shipping_tk_next_countries[next_country.code]['couriers'][courier.id]['payer'] is defined and shipping_tk_next_countries[next_country.code]['couriers'][courier.id]['payer'] == 'receiver' %}checked="checked"{% endif %}/>
																			<span class="btn btn-default">{{ text_recipient }}</span>
																		</label>
																		<label class="button-label">
																			<input type="radio" name="shipping_tk_next_countries[{{ next_country.code }}][couriers][{{ courier.id }}][payer]" value="sender" {% if shipping_tk_next_countries[next_country.code]['couriers'][courier.id]['payer'] is not defined or shipping_tk_next_countries[next_country.code]['couriers'][courier.id]['payer'] == 'sender' %}checked="checked"{% endif %}/>
																			<span class="btn btn-default">{{ text_sender }}</span>
																		</label>
																	</div>
																</div>
																
																<div class="form-group">
																	<label class="col-sm-3 control-label">{{ text_included_shipping_price }}</label>
																	<div class="col-sm-9">
																		{% if shipping_tk_next_countries[next_country.code]['couriers'][courier.id]['included_shipping_price'] %}
