{{ header }}{{ column_left }} 
{% set weight_value_row = 0 %}
<div id="content">
	<div class="page-header">
		<div class="container-fluid">
			<div class="pull-right">
				{% if (shipping_tk_sameday_logged) %}
					<button type="submit" form="form-sameday" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary">
						<i class="fa fa-save"></i></button>
				{% endif %}
				<a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a>
			</div>
			<h1>{{ heading_title }}</h1>
			<ul class="breadcrumb">
				{% for breadcrumb in breadcrumbs %}
					<li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
				{% endfor %}
			</ul>
		</div>
	</div>
	<div class="container-fluid">
		
		{% if (error_warning) %}
			<div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
				<button type="button" class="close" data-dismiss="alert">&times;</button>
			</div>
		{% endif %}
		
		{% if (success) %}
			<div class="alert alert-success alert-dismissible"><i class="fa fa-check-circle"></i> {{ success }}
				<button type="button" class="close" data-dismiss="alert">&times;</button>
			</div>
		{% endif %}
		
		<form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-sameday" class="form-horizontal">
			
			{% if module_tk_checkout_token %}
				
				{% if shipping_tk_sameday_logged %}
					<div class="panel panel-default">
						<div class="panel-body">
							<div class="form-group required">
								<label class="col-sm-3 control-label">Token | expire</label>
								<div class="col-sm-9">
									{{ tk_sameday_token }} | {{ tk_sameday_expire }}
								</div>
							</div>
							
							<div class="form-group required">
								<label class="col-sm-3 control-label">USERNAME</label>
								<div class="col-sm-9">
									<input type="text" name="shipping_tk_sameday_username" value="{{ shipping_tk_sameday_username }}" placeholder="USERNAME" class="form-control" disabled/>
								</div>
							</div>
							<div class="form-group required">
								<label class="col-sm-3 control-label">PASSWORD</label>
								<div class="col-sm-9">
									<input type="password" name="shipping_tk_sameday_password" value="{{ shipping_tk_sameday_password }}" placeholder="PASSWORD" class="form-control" disabled/>
								</div>
							</div>
							<div class="form-group required">
								<label class="col-sm-3 control-label">TEST MOD</label>
								<div class="col-sm-9">
									<input type="text" name="shipping_tk_sameday_test" value="{{ shipping_tk_sameday_test }}" placeholder="TEST MOD" class="form-control" disabled/>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label"></label>
								<div class="col-sm-9">
									<a onclick="samedayButtonLogout();" class="btn btn-primary"><span>{{ button_logout }}</span></a>
								</div>
							</div>
							<hr>
							<div class="form-group">
								<div class="col-sm-3 text-right"><p><b>{{ text_cron_lockers }}:</b></p></div>
								<div class="col-sm-9">
									<p>/opt/cpanel/ea-php74/root/usr/bin/php {{ root }}
										catalog/controller/tk_checkout/cron_sameday.php {{ web_url }} > /dev/null 2>&1</p>
								</div>
							</div>
							<div class="form-group">
								<div class="col-sm-3 text-right"><p><b>{{ text_cron_shipping }}:</b></p></div>
								<div class="col-sm-9">
									<p>/opt/cpanel/ea-php74/root/usr/bin/php {{ root }}
										catalog/controller/tk_checkout/cron_sameday_shipping.php {{ web_url }} > /dev/null 2>&1</p>
								</div>
							</div>
						</div>
					</div>
				{% endif %}
				
				{% if not shipping_tk_sameday_logged %}
					
					<div class="panel panel-default">
						<div class="panel-body">
							<div class="form-group required">
								<label class="col-sm-3 control-label" for="shipping_tk_sameday_username">USERNAME</label>
								<div class="col-sm-9">
									<input type="text" id="shipping_tk_sameday_username" name="shipping_tk_sameday_username" value="{{ shipping_tk_sameday_username }}" placeholder="USERNAME" class="form-control"/>
									{% if (error_username) %}
										<span class="text-danger">{{ error_username }}</span>
									{% endif %}
								</div>
							</div>
							<div class="form-group required">
								<label class="col-sm-3 control-label" for="shipping_tk_sameday_password">PASSWORD</label>
								<div class="col-sm-9">
									<input type="password" id="shipping_tk_sameday_password" name="shipping_tk_sameday_password" value="{{ shipping_tk_sameday_password }}" placeholder="PASSWORD" class="form-control"/>
									{% if (error_password) %}
										<span class="text-danger">{{ error_password }}</span>
									{% endif %}
								</div>
							</div>
							<div class="form-group required">
								<label class="col-sm-3 control-label" for="shipping_tk_sameday_test">TEST MOD</label>
								<div class="col-sm-9">
									<select class="form-control" id="shipping_tk_sameday_test" name="shipping_tk_sameday_test">
										{% if (shipping_tk_sameday_test) %}
											<option value="1" selected="selected">{{ text_enabled }}</option>
											<option value="0">{{ text_disabled }}</option>
										{% else %}
											<option value="1">{{ text_enabled }}</option>
											<option value="0" selected="selected">{{ text_disabled }}</option>
										{% endif %}
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label"></label>
								<div class="col-sm-9">
									<a id="login_button" onclick="samedayButtonLogin();" class="btn btn-primary"><span id="login_text">{{ button_login }}</span></a>
									<div id="login_error"></div>
								</div>
							</div>
						</div>
					</div>
				
				{% else %}
					
					<div class="panel panel-default">
						<div class="panel-body" id="additional_table">
							
							<div class="col-sm-2">
								<ul class="nav nav-pills nav-stacked">
									<li class="active">
										<a href="#v-pills-settings" data-toggle="tab">{{ text_settings }}</a>
									</li>
									{% if sameday_countries %}
									<li>
										<a href="#v-pills-statuses" data-toggle="tab">{{ text_statuses }}</a>
									</li>
									{% for sameday_country in sameday_countries %}
										<li>
											<a href="#v-pills-{{ sameday_country.code }}" data-toggle="tab">{{ sameday_country.country }}</a>
										</li>
									{% endfor %}
									{% endif %}
								</ul>
							</div>
							
							<div class="col-sm-10">
								<div class="tab-content">
									
									<div class="tab-pane fade in active" id="v-pills-settings">
										
										<div class="form-group">
											<div class="col-sm-3 control-label">
												<a onclick="samedayPickup();" class="btn btn-info" id="button_pickup"><span>{{ button_sinc_pickup_point }}</span></a>
												<div class="button_pickup_error"></div>
											</div>
											<div class="col-sm-9">
												<div class="table-responsive">
													<table class="table table-bordered table-hover">
														<thead>
														<tr>
															<td class="text-left">{{ column_samedayId }}</td>
															<td class="text-left">{{ column_alias }}</td>
															<td class="text-left">{{ column_city }}</td>
															<td class="text-left">{{ column_county }}</td>
															<td class="text-left">{{ column_address }}</td>
															<td class="text-left">{{ column_default_address }}</td>
														</tr>
														</thead>
														<tbody>
														{% if sameday_pickups %}
															{% for pickup in sameday_pickups %}
																<tr>
																	<td>{{ pickup.sameday_id }}</td>
																	<td>{{ pickup.sameday_alias }}</td>
																	<td>{{ pickup.city }}</td>
																	<td>{{ pickup.county }}</td>
																	<td>{{ pickup.address }}</td>
																	<td>{{ pickup.default_pickup_point == 1 ? text_yes : text_no }}</td>
																</tr>
															{% endfor %}
														{% else %}
															<tr>
																<td class="text-center" colspan="5">{{ text_pickup_points_empty }}</td>
															</tr>
														{% endif %}
														</tbody>
													</table>
												</div>
											</div>
										</div>
					
										{% if sameday_countries %}
										{% if sameday_pickups %}
											<div class="form-group">
												<label class="col-sm-3 control-label" for="shipping_tk_sameday_pickup">{{ text_sameday_pickup }}</label>
												<div class="col-sm-9">
													<select class="form-control" id="shipping_tk_sameday_pickup" name="shipping_tk_sameday_pickup">
														{% for pickup in sameday_pickups %}
															<option value="{{ pickup.sameday_id }}" {% if (pickup.sameday_id == shipping_tk_sameday_pickup) %}selected="selected"{% endif %}>{{ pickup.sameday_id }} - {{ pickup.sameday_alias }} - {{ pickup.address }}</option>
														{% endfor %}
													</select>
												</div>
											</div>
											
											{% if shipping_tk_sameday_pickup and sameday_contact_persons %}
											<div class="form-group">
												<label class="col-sm-3 control-label" for="shipping_tk_sameday_contact_person">{{ text_sameday_contact_persons }}</label>
												<div class="col-sm-9">
													<select class="form-control" id="shipping_tk_sameday_contact_person" name="shipping_tk_sameday_contact_person">
														{% for person in sameday_contact_persons[shipping_tk_sameday_pickup] %}
															<option value="{{ person.id }}" {% if (person.id == shipping_tk_sameday_contact_person) %}selected="selected"{% endif %}>{{ person.id }} - {{ person.name }} - {{ person.phoneNumber }}</option>
														{% endfor %}
													</select>
												</div>
											</div>
											{% endif %}
										{% endif %}
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="shipping_tk_sameday_package_type">{{ text_package_type }}</label>
											<div class="col-sm-9">
												<select class="form-control" id="shipping_tk_sameday_package_type" name="shipping_tk_sameday_package_type">
													<option value="0" {% if (shipping_tk_sameday_package_type == 0) %}selected="selected"{% endif %}>Package</option>
													<option value="1" {% if (shipping_tk_sameday_package_type == 1) %}selected="selected"{% endif %}>Envelope</option>
													<option value="2" {% if (shipping_tk_sameday_package_type == 2) %}selected="selected"{% endif %}>Large Package</option>
												</select>
											</div>
										</div>
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="shipping_tk_sameday_shipment_type">{{ text_shipment_type }}</label>
											<div class="col-sm-9">
												<select class="form-control" id="shipping_tk_sameday_shipment_type" name="shipping_tk_sameday_shipment_type">
													<option value="A6" {% if (shipping_tk_sameday_shipment_type == 'A6') %}selected="selected"{% endif %}>A6</option>
													<option value="A4" {% if (shipping_tk_sameday_shipment_type == 'A4') %}selected="selected"{% endif %}>A4</option>
												</select>
											</div>
										</div>
										
										{% if custom_fields %}
											<div class="form-group">
												<label class="col-sm-3 control-label" for="shipping_tk_sameday_company">{{ text_field_company }}</label>
												<div class="col-sm-9">
													<select name="shipping_tk_sameday_company" id="shipping_tk_sameday_company" class="form-control">
														<option value="9999">{{ text_no_company}}</option>
														{% for custom_field in custom_fields %}
															{% if custom_field.custom_field_id == shipping_tk_sameday_company %}
																<option value="{{ custom_field.custom_field_id }}" selected="selected">{{ custom_field.name }}</option>
															{% else %}
																<option value="{{ custom_field.custom_field_id }}">{{ custom_field.name }}</option>
															{% endif %}
														{% endfor %}
													</select>
												</div>
											</div>
										{% endif %}
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="shipping_tk_sameday_geo_zone_id">{{ text_geo_zone }}</label>
											<div class="col-sm-9">
												<select class="form-control" id="shipping_tk_sameday_geo_zone_id" name="shipping_tk_sameday_geo_zone_id">
													<option value="0">{{ text_all_zones }}</option>
													{% for geo_zone in geo_zones %}
														{% if (geo_zone['geo_zone_id'] == shipping_tk_sameday_geo_zone_id) %}
															<option value="{{ geo_zone['geo_zone_id'] }}" selected="selected">{{ geo_zone['name'] }}</option>
														{% else %}
															<option value="{{ geo_zone['geo_zone_id'] }}">{{ geo_zone['name'] }}</option>
														{% endif %}
													{% endfor %}
												</select>
											</div>
										</div>
											
											<div class="form-group">
												<label class="col-sm-3 control-label" for="input-tax-class">Данъчен клас</label>
												<div class="col-sm-9">
													<select name="shipping_tk_sameday_tax_class_id" id="input-tax-class" class="form-control">
														<option value="0">{{ text_none }}</option>
														{% for tax_class in tax_classes %}
															{% if tax_class.tax_class_id == shipping_tk_sameday_tax_class_id %}
																<option value="{{ tax_class.tax_class_id }}" selected="selected">{{ tax_class.title }}</option>
															{% else %}
																<option value="{{ tax_class.tax_class_id }}">{{ tax_class.title }}</option>
															{% endif %}
														{% endfor %}
													</select>
												</div>
											</div>
										
										{% endif %}
										
										<div class="form-group">
											<label class="col-sm-3 control-label" >{{ text_send_country }}</label>
											<div class="col-sm-9">
												<select class="form-control" name="shipping_tk_sameday_send_country">
													{% for sameday_country in sameday_countries %}
														<option value="{{ sameday_country.code }}" {% if (shipping_tk_sameday_send_country == sameday_country.code) %}selected="selected"{% endif %}>{{ sameday_country.country }}</option>
													{% endfor %}
												</select>
											</div>
										</div>
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="shipping_tk_sameday_sort_order">{{ text_sort_order }}</label>
											<div class="col-sm-9">
												<input class="form-control" type="text" id="shipping_tk_sameday_sort_order" name="shipping_tk_sameday_sort_order" value="{{ shipping_tk_sameday_sort_order }}" size="1"/>
											</div>
										</div>
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="shipping_tk_sameday_status">{{ text_debug }}</label>
											<div class="col-sm-9">
												<select class="form-control" id="shipping_tk_sameday_debug" name="shipping_tk_sameday_debug">
													{% if (shipping_tk_sameday_debug) %}
														<option value="1" selected="selected">{{ text_enabled }}</option>
														<option value="0">{{ text_disabled }}</option>
													{% else %}
														<option value="1">{{ text_enabled }}</option>
														<option value="0" selected="selected">{{ text_disabled }}</option>
													{% endif %}
												</select>
											</div>
										</div>
										
										<div class="form-group">
											<label class="col-sm-3 control-label" for="shipping_tk_sameday_status">{{ text_status }}</label>
											<div class="col-sm-9">
												<select class="form-control" id="shipping_tk_sameday_status" name="shipping_tk_sameday_status">
													{% if (shipping_tk_sameday_status) %}
														<option value="1" selected="selected">{{ text_enabled }}</option>
														<option value="0">{{ text_disabled }}</option>
													{% else %}
														<option value="1">{{ text_enabled }}</option>
														<option value="0" selected="selected">{{ text_disabled }}</option>
													{% endif %}
												</select>
											</div>
										</div>
									
									</div>
									{% if sameday_countries %}
									<div class="tab-pane" id="v-pills-statuses">
										
										<div class="form-group">
											<label class="col-sm-3 control-label">{{ text_statuses_10 }}:</label>
											<div class="col-sm-9">
												<select name="shipping_tk_sameday_order_status_id" class="form-control">
													<option value="0">{{ text_statuses_9 }}</option>
													{% for order_status in order_statuses %}
														
														{% if (order_status['order_status_id'] == shipping_tk_sameday_order_status_id) %}
															<option value="{{ order_status['order_status_id'] }}" selected="selected">{{ order_status['name'] }}</option>
														{% else %}
															<option value="{{ order_status['order_status_id'] }}">{{ order_status['name'] }}</option>
														{% endif %}
													{% endfor %}
												</select>
											</div>
										</div>
										
										<div class="form-group">
											<label class="col-sm-3 control-label">{{ text_statuses_8 }}:</label>
											<div class="col-sm-9">
												<select name="shipping_tk_sameday_order_status_id_mail" class="form-control">
													{% if  shipping_tk_sameday_order_status_id_mail %}
														<option value="0">{{ text_disabled }}</option>
														<option value="1" selected="selected">{{ text_enabled }}</option>
													{% else %}
														<option value="0" selected="selected">{{ text_disabled }}</option>
														<option value="1">{{ text_enabled }}</option>
													{% endif %}
												</select>
											</div>
										</div>
										
										<div class="form-group">
											<label class="col-sm-3 control-label">{{ text_statuses_1 }}<br> {shipment_number}
												- {{ text_statuses_2 }}:</label>
											<div class="col-sm-9">
												<textarea name="shipping_tk_sameday_order_status_id_mail_text" rows="3" class="form-control">{{ shipping_tk_sameday_order_status_id_mail_text }}</textarea>
											</div>
										</div>
										
										<div class="form-group">
											<div class="col-sm-3 text-right"></div>
											<div class="col-sm-9">
												<br><br><h4><b>{{ text_statuses_3 }}</b></h4>
											</div>
										</div>
										
										{% for sameday_text_status in sameday_statuses.data %}
											<div class="form-group">
												<label class="col-sm-3 control-label">{{ sameday_text_status.id }} - {{ sameday_text_status.label }}</label>
												
												<div class="col-sm-9">
													<div class="row">
														<div class="col-sm-3">{{ text_statuses_4 }}</div>
														<div class="col-sm-3">
															<select name="shipping_tk_sameday_order_status[{{ sameday_text_status.id }}]" class="form-control">
																<option value="">{{ text_statuses_5 }}</option>
																{% for order_status in order_statuses %}
																	{% if  shipping_tk_sameday_order_status[sameday_text_status.id] == order_status.order_status_id %}
																		<option value="{{ order_status.order_status_id }}" selected="selected">{{ order_status.name }}</option>
																	{% else %}
																		<option value="{{ order_status.order_status_id }}">{{ order_status.name }}</option>
																	{% endif %}
																{% endfor %}
															</select>
														</div>
														<div class="col-sm-3">{{ text_statuses_6 }}</div>
														<div class="col-sm-3">
															<select name="shipping_tk_sameday_order_status_mail[{{ sameday_text_status.id }}]" class="form-control">
																{% if  shipping_tk_sameday_order_status_mail[sameday_text_status.id] %}
																	<option value="0">{{ text_disabled }}</option>
																	<option value="1" selected="selected">{{ text_enabled }}</option>
																{% else %}
																	<option value="0" selected="selected">{{ text_disabled }}</option>
																	<option value="1">{{ text_enabled }}</option>
																{% endif %}
															</select>
														</div>
														<div class="clearfix"></div>
														<br>
														
														<div class="col-sm-3">{{ text_statuses_7 }}</div>
														<div class="col-sm-9">
															<textarea name="shipping_tk_sameday_order_status_mail_text[{{ sameday_text_status.id }}]" rows="3" placeholder="{{ text_add_text }}" class="form-control">{{ shipping_tk_sameday_order_status_mail_text[sameday_text_status.id] }}</textarea>
														</div>
													</div>
												</div>
											</div>
										{% endfor %}
									
									</div>
									
									{% for sameday_country in sameday_countries %}
										<div class="tab-pane fade" id="v-pills-{{ sameday_country.code }}">
											
											<div class="form-group">
												<div class="col-sm-12">
													<h3>{{ sameday_country.country }}
														<small>{{ text_code }}:{{ sameday_country.code }}</small> <small>{{ text_currency }}
															:{{ sameday_country.currency }}</small>
													</h3>
												</div>
											</div>
											
											<input type="hidden" name="shipping_tk_sameday_countries[{{ sameday_country.code }}][id]" value="{{ sameday_country.country_id }}"/>
											<input type="hidden" name="shipping_tk_sameday_countries[{{ sameday_country.code }}][code]" value="{{ sameday_country.code }}"/>
											<input type="hidden" name="shipping_tk_sameday_countries[{{ sameday_country.code }}][currency]" value="{{ sameday_country.currency }}"/>
											<input type="hidden" name="shipping_tk_sameday_countries[{{ sameday_country.code }}][country]" value="{{ sameday_country.country }}"/>
											
											<div class="form-group">
												<label class="col-sm-3 control-label">{{ text_status }}</label>
												<div class="col-sm-9">
													{% if shipping_tk_sameday_countries[sameday_country.code]['status'] %}
														<label class="button-label">
															<input type="radio" name="shipping_tk_sameday_countries[{{ sameday_country.code }}][status]" value="0" class="input-{{ sameday_country.code }}-status"/>
															<span class="btn btn-default">{{ text_disabled }}</span> </label>
														<label class="button-label">
