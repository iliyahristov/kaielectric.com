<script>
    $(document).delegate('select[name=\'shipping_tk_sameday_pickup\']', 'change', function () {
        $("#form-sameday").submit();
    });

	{% for sameday_country in sameday_countries %}
  $('.input-{{ sameday_country.code }}-status').on('change', function () {
      if ($(this).val() > 0) {
          $('#box-{{ sameday_country.code }}-status').fadeIn();
      } else {
          $('#box-{{ sameday_country.code }}-status').fadeOut();
      }
  });
	
	{% for service in sameday_services.data %}
  $('.input-{{ sameday_country.code }}-{{ service.id }}-status').on('change', function () {
      if ($(this).val() > 0) {
          $('#box-{{ sameday_country.code }}-{{ service.id }}-status').fadeIn();
      } else {
          $('#box-{{ sameday_country.code }}-{{ service.id }}-status').fadeOut();
      }
  });

  $('.input-{{ sameday_country.code }}-{{ service.id }}-option').on('change', function () {
      if ($(this).val() != 0) {
          $('#box-{{ sameday_country.code }}-{{ service.id }}-option').fadeIn();
      } else {
          $('#box-{{ sameday_country.code }}-{{ service.id }}-option').fadeOut();
      }
  });

  $('.input-{{ sameday_country.code }}-{{ service.id }}-status').on('change', function () {
      if ($(this).val() != 0) {
          $('#box-{{ sameday_country.code }}-{{ service.id }}-status').fadeIn();
      } else {
          $('#box-{{ sameday_country.code }}-{{ service.id }}-status').fadeOut();
      }
  });

  $('.input-{{ sameday_country.code }}-{{ service.id }}-office-status').on('change', function () {
      if ($(this).val() != 0) {
          $('#box-{{ sameday_country.code }}-{{ service.id }}-office-status').fadeIn();
      } else {
          $('#box-{{ sameday_country.code }}-{{ service.id }}-office-status').fadeOut();
      }
  });

  $('.input-{{ sameday_country.code }}-{{ service.id }}-machine-status').on('change', function () {
      if ($(this).val() != 0) {
          $('#box-{{ sameday_country.code }}-{{ service.id }}-machine-status').fadeIn();
      } else {
          $('#box-{{ sameday_country.code }}-{{ service.id }}-status-status').fadeOut();
      }
  });
	{% endfor %}
	{% endfor %}

  function samedayLocers(country, page) {
      $('#button_lockers_error_' + country).html('').removeClass("text-danger");
      $('#button_lockers_' + country).attr('disabled', true);
		  
      if(page == 1){
        $('#button_lockers_' + country).after('<span id="loading_lockers_'+ country + '" class="attention" style="padding: 5px;">page: ' + page + '</span>');
      }

      $.ajax({
          url: 'index.php?route=extension/shipping/tk_sameday/lockers&user_token={{ user_token }}',
          type: 'POST',
          data: 'country=' + encodeURIComponent(country) + '&page=' + encodeURIComponent(page),
          dataType: 'json',
          success: function (data) {
              if (data) {
                  $('#loading').remove();

                  if (data.success) {
                      window.location = window.location;
                  } else if (data.setting) {
                      $('#loading_lockers_' + country).remove();
                      $('#button_lockers_' + country).after('<span id="loading_lockers_'+ country + '" class="attention" style="padding: 5px;">page:' + data.setting.page + ' from:' + data.setting.pages + '</span>');
                      
                      samedayLocers(country, data.setting.page)
                  } else {
                      $('#loading_lockers_' + country).remove();
                      $('#button_lockers_' + country).attr('disabled', false);
                      $('#button_lockers_error_' + country).html(data.message).addClass('text-danger');
                  }
              }
          },
          error: function (request) {
              $('#loading_lockers_' + country).remove();

              $('#button_lockers_error_' + country).html('Error').addClass("text-danger");
          }
      });
  }

  function samedayPickup() {
      $('#pickup_loading').remove();
      $('#button_pickup_error').html('').removeClass("text-danger");
      $('#button_pickup').after('<span id="pickup_loading" class="attention" style="padding: 5px;">loading...</span>');

      $.ajax({
          url: 'index.php?route=extension/shipping/tk_sameday/pickup&user_token={{ user_token }}',
          type: 'POST',
          data: '',
          dataType: 'json',
          success: function (data) {
              if (data) {
                  $('#pickup_loading').remove();

                  if (data.success) {
                      window.location = window.location;
                  } else {
                      $('#button_pickup_error').html(data.message).addClass('text-danger');
                  }
              }
          },
          error: function (request) {
              $('#pickup_loading').remove();

              $('#button_pickup_error').html('Error').addClass("text-danger");
          }
      });
  }

  function samedayButtonLogin() {
     
      $('#login_error').html('').removeClass("text-danger");
      $('#login_button').after('<span id="loading" class="attention" style="padding: 5px;">loading...</span>');

      $.ajax({
          url: 'index.php?route=extension/shipping/tk_sameday/login&user_token={{ user_token }}',
          type: 'POST',
          data: 'username=' + encodeURIComponent($('#shipping_tk_sameday_username').val()) + '&password=' + encodeURIComponent($('#shipping_tk_sameday_password').val()) + '&test=' + encodeURIComponent($('#shipping_tk_sameday_test').val()),
          dataType: 'json',
          success: function (data) {
              if (data) {
                  $('#loading').remove();

                  if (data.message) {
                      $('#login_error').html(data.message).addClass('text-danger');
                  } else if (data.success) {
                      window.location = window.location;
                  } else {
                      $('#login_error').html('Error - 1!').addClass('text-danger');
                  }
              }
          },
          error: function (request) {
		          $('#loading').remove();

              $('#login_error').html('Login Error').addClass("text-danger");
          }
      });
  }

  function samedayButtonLogout() {
      if (confirm('{{ text_warning_logout }}')) {
          $.get('index.php?route=extension/shipping/tk_sameday/logout&user_token={{ user_token }}').done(function () {
              window.location = window.location;
          });
      }
  }

</script>
