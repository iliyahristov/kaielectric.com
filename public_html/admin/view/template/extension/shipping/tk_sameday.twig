                                                html += '  <td class="text-right"><input type="text" name="shipping_tk_sameday_countries[{{ sameday_country.code }}][services][{{ service.id }}][weight_price][' + weight_value_row_{{ service.id }} + '][to]" value="" placeholder="0.00" class="form-control" /></td>';
                                                html += '  <td class="text-right"><input type="text" name="shipping_tk_sameday_countries[{{ sameday_country.code }}][services][{{ service.id }}][weight_price][' + weight_value_row_{{ service.id }} + '][price]" value="" placeholder="0.00" class="form-control" /></td>';
                                                html += '  <td class="text-right"><button type="button" onclick="$(\'#weight-value-row' + weight_value_row_{{ service.id }} + '\').remove();" data-toggle="tooltip" title="{{ button_remove }}" class="btn btn-danger"><i class="fa fa-minus-circle"></i></button></td>';
                                                html += '</tr>';

                                                $('#weight-value-{{ sameday_country.code }}-{{ service.id }}-address tbody').append(html);

                                                weight_value_row_{{ service.id }}++;
                                            }
																				
																				</script>
																				
																		</div>
																	
																</div>
															
															</div>
														</div>
